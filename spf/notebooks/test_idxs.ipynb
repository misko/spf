    "import torch\n",
    "\n",
    "\n",
    "def to_bin(x, bins):\n",
    "    return ((x / (2 * torch.pi) + 0.5) * bins).to(torch.long)\n",
    "\n",
    "\n",
    "to_bin(torch.linspace(-torch.pi, torch.pi, 100))"
   ]
